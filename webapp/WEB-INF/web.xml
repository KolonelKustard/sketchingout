<!DOCTYPE web-app 
  PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" 
  "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <servlet>
    <servlet-name>Tester</servlet-name>
    <servlet-class>com.totalchange.consequences.Tester</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Tester</servlet-name>
    <url-pattern>/tester</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>Consequences</servlet-name>
    <servlet-class>com.totalchange.consequences.ConsequencesServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Consequences</servlet-name>
    <url-pattern>/consequences</url-pattern>
  </servlet-mapping>

</web-app>